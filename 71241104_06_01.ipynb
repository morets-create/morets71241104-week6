        "def perkalian(a, b):\n",
        "    hasil = 0\n",
        "    for _ in range(a):\n",
        "        hasil += b\n",
        "    return hasil\n",
        "\n",
        "\n",
        "print(perkalian(6, 5))\n",
        "print(perkalian(7, 10))"
